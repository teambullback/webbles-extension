{
  "manifest_version": 2,
  "name": "goDumber",
  "description": "goDumber is a free & fast & easy-to-use chrome extension which helps people to easily accomplish their daily IT chores",
  "version": "0.0.1",
  "permissions":[
    "tabs", "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
